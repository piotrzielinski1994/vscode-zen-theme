        "foreground": "#CC7832",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#CC7832",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#A9B7C6"
      }
